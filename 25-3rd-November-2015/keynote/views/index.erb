    <style type="text/css">
      @-webkit-keyframes slidetounlock {
        0% {
          background-position: -720px 0;
        }
        100% {
          background-position: 720px 0;
        }
      }

      @-webkit-keyframes spinningslidetounlock {
        0% {
          background-position: -720px 0;
        }
        100% {
          background-position: 720px 0;
          -webkit-transform: rotate(360deg);
        }
